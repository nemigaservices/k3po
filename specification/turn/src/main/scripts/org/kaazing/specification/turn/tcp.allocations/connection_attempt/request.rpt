property connectURI "tcp://localhost:3478"

property reqXorRelayedAddressIp ${turn:ipXOR("192.0.2.15")}
property reqXorRelayedAddressPort ${turn:portXOR(8080)}

connect ${connectURI}
connected


#ConnectionAttempt Indication from server to client
#STUN Message type | Message Length
read [0x00 0x1c 0x00 0x3c]
read [0x21 0x12 0xa4 0x42]
read [0xaa 0x5c 0x56 0xc2 0xb5 0xbe 0xb4 0xbe 0x8f 0x7d 0xfe 0x2a]

#Attribute CONNECTION-ID
read [0x00 0x2a 0x00 0x04]
read [0xec 0x94 0xe0 0x00]

#Attribute XOR-PEER-ADDRESS
read [0x00 0x12 0x00 0x08]
read [0x00 0x01] ${reqXorRelayedAddressPort} ${reqXorRelayedAddressIp}

#Attribute SOFTWARE
read [0x80 0x22 0x00 0x1a]
read [0x43 0x6f 0x74 0x75 0x72 0x6e 0x2d 0x34 0x2e 0x35 0x2e 0x30 0x2e 0x33 0x20 0x27 0x64 0x61 0x6e 0x20 0x45 0x69 0x64 0x65 0x72 0x27 0x00 0x04]

#Attribute FINGERPRINT
read [0x80 0x28 0x00 0x04]
read [0x4e 0x7c 0xf3 0x23]


