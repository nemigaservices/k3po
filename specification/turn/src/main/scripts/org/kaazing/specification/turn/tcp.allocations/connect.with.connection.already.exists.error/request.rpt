property connectURI "tcp://localhost:3478"

property reqXorRelayedAddressIp ${turn:ipXOR("192.0.2.15")}
property reqXorRelayedAddressPort ${turn:portXOR(8080)}

connect ${connectURI}
connected


#Connect Request
#STUN Message type | Message Length
write [0x00 0x0A 0x00 0x5C]
write [0x21 0x12 0xA4 0x42]
write ${turn:generateTransactionId()}

#Attribute XOR-Peer-Address
write [0x00 0x12 0x00 0x08]
write [0x00 0x01] ${reqXorRelayedAddressPort} ${reqXorRelayedAddressIp}

#Attribute Username
write [0x00 0x06 0x00 0x0E]
write [0x31 0x34 0x37 0x33 0x34 0x34 0x30 0x36 0x33 0x36 0x3A 0x62 0x6F 0x62 0x00 0x00]

#Attribute Nonce
write [0x00 0x15 0x00 0x10]
write [0x61 0x62 0x39 0x33 0x30 0x35 0x36 0x39 0x31 0x31 0x64 0x31 0x30 0x36 0x64 0x63]

#Attribute Realm
write [0x00 0x14 0x00 0x04]
write [0x64 0x65 0x6D 0x6F]

#Attribute Message-Integrity
write [0x00 0x08 0x00 0x14]
write [0xC1 0xEF 0x2A 0xAC 0xB3 0x90 0xFE 0x69 0xC4 0x4E 0xAA 0xE7 0x6F 0xD1 0x63 0x89 0x7F 0xA3 0x96 0x27]

# Attribute fingerprint
write [0x80 0x28 0x00 0x04]
write [0xD4 0x2D 0xB9 0xA9]


#Connect Request
#STUN Message type | Message Length
write [0x00 0x0A 0x00 0x5C]
write [0x21 0x12 0xA4 0x42]
write ${turn:generateTransactionId()}

#Attribute XOR-Peer-Address
write [0x00 0x12 0x00 0x08]
write [0x00 0x01] ${reqXorRelayedAddressPort} ${reqXorRelayedAddressIp}

#Attribute Username
write [0x00 0x06 0x00 0x0E]
write [0x31 0x34 0x37 0x33 0x34 0x34 0x30 0x36 0x33 0x36 0x3A 0x62 0x6F 0x62 0x00 0x00]

#Attribute Nonce
write [0x00 0x15 0x00 0x10]
write [0x61 0x62 0x39 0x33 0x30 0x35 0x36 0x39 0x31 0x31 0x64 0x31 0x30 0x36 0x64 0x63]

#Attribute Realm
write [0x00 0x14 0x00 0x04]
write [0x64 0x65 0x6D 0x6F]

#Attribute Message-Integrity
write [0x00 0x08 0x00 0x14]
write [0xC1 0xEF 0x2A 0xAC 0xB3 0x90 0xFE 0x69 0xC4 0x4E 0xAA 0xE7 0x6F 0xD1 0x63 0x89 0x7F 0xA3 0x96 0x27]

# Attribute fingerprint
write [0x80 0x28 0x00 0x04]
write [0xD4 0x2D 0xB9 0xA9]


##446 Connection Already Exists error response
#STUN Header
read [0x01] [0x13] [0x00] [0x08] #Type|Length
read [0x21] [0x12] [0xa4] [0x42] #Magic Cookie
read [0..12]
#Attribute ERROR-CODE
read [0x00] [0x09] [0x00] [0x04]
read [0x00] [0x00] [0x04] [0x2E]
