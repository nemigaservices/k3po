connect http://localhost:8080/
connected

write method "GET"
write header host
write header "Content-Type" "text/plain"
write header content-length
write close

read status "200" "OK"
read header "Content-Type" "text/plain"
read header "Transfer-Encoding" "chunked"
read header "Trailer" "Connection"
read "Chunk A"
read "Chunk B"
read "Chunk C"
read trailer "Connection" "close"
read closed

